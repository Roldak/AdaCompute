with "opencl";

project AdaCompute is
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";

   for Main use ("main.adb");

   package Compiler renames OpenCL.Compiler;

   package Builder is
      for Default_Switches ("Ada") use ("-m");
   end Builder;
end AdaCompute;
